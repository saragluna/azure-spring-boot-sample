<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <parent>
    <artifactId>spring-cloud-azure-samples</artifactId>
    <groupId>com.azure.spring</groupId>
    <version>1.0.0</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <artifactId>spring-cloud-azure-starter-sample</artifactId>

  <properties>
    <azure.spring.version>4.0.0-beta.1</azure.spring.version>
    <azure.cosmos.version>4.20.0</azure.cosmos.version>
    <azure.app.configuration.version>1.2.3</azure.app.configuration.version>
    <azure.servicebus.version>7.4.1</azure.servicebus.version>
    <azure.eventhubs.version>5.10.1</azure.eventhubs.version>
    <azure.keyvault.secret.version>4.3.4</azure.keyvault.secret.version>
    <azure.keyvault.certificate.version>4.2.4</azure.keyvault.certificate.version>
    <azure.storage.blob.version>12.14.1</azure.storage.blob.version>
    <azure.storage.file.version>12.11.1</azure.storage.file.version>
    <azure.storage.queue.version>12.11.1</azure.storage.queue.version>
  </properties>

  <dependencies>
    <dependency>
      <groupId>com.azure.spring</groupId>
      <artifactId>spring-cloud-azure-starter</artifactId>
      <version>${azure.spring.version}</version>
    </dependency>
    <dependency>
      <groupId>com.azure.spring</groupId>
      <artifactId>spring-cloud-azure-starter-actuator</artifactId>
